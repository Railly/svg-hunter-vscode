{
  "name": "svg-hunter",
  "displayName": "svg-hunter",
  "description": "Hunt SVGs inside your code editor",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:svg-hunter.insertSvgHtml",
    "onCommand:svg-hunter.insertRawJsxSvg",
    "onCommand:svg-hunter.createJsxSvgComponent"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "svg-hunter.insertSvgHtml",
        "category": "SVG Hunter",
        "title": "Insert SVG as plain HTML"
      },
      {
        "command": "svg-hunter.insertRawJsxSvg",
        "category": "SVG Hunter",
        "title": "Insert SVG as JSX"
      },
      {
        "command": "svg-hunter.createJsxSvgComponent",
        "category": "SVG Hunter",
        "title": "Create new JSX component from SVG"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "update-data": "bash ./scripts/update-data.sh",
    "vscode:prepublish": "npm run update-data && npm run compile"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/babel__template": "^7.4.1",
    "@types/glob": "^8.0.0",
    "@types/lodash.memoize": "^4.1.7",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/prettier": "^2.7.2",
    "@types/svgo": "2.6.4",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@svgr/core": "^6.5.1",
    "@svgr/plugin-jsx": "^6.5.1",
    "@svgr/plugin-prettier": "^6.5.1",
    "@svgr/plugin-svgo": "^6.5.1",
    "axios": "^1.2.3",
    "lodash.memoize": "^4.1.2",
    "svgo": "2.8.0"
  }
}
